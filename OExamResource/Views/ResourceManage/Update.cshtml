@{
    ViewBag.Title = "更新资源库";
    Layout = "~/Views/Shared/_Back.cshtml";
}
@section SubHead{
    <script src="@Url.Content("~/Scripts/views/manage/updateresource.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/plugin/jquery.form.js")" type="text/javascript"></script>
}
<h2>
    更新资源库</h2>
<div style="margin: 0 100px">
    <form id="form_update" style="padding: 20px;" action="/ResourceManage/UpdateResource"
    method="post" enctype="multipart/form-data">
    <table border="0" cellpadding="0" cellspacing="10" width="100%">
        <tr>
            <td style="height: 30px" width="30%">
                表单名称：<br />
                要导入的Excel文件中第一个表单的名称
            </td>
            <td>
                <input type="text" name="txtSheet" id="txtSheet" value="sheet1" />
            </td>
        </tr>
        <tr>
            <td style="height: 30px">
                文件路径：<br />
                要导入的Excel文件所在的物理路径
            </td>
            <td>
                <input name="upXls" id="upXls" type="file" />
            </td>
        </tr>
        <tr>
            <td>
            </td>
            <td>
                <input type="submit" name="btnImport" id="btnImport" value="导入" />
            </td>
        </tr>
    </table>
    </form>
</div>