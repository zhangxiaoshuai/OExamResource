<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>@* <script src="@Url.Content("~/Scripts/common/ie6.js")" type="text/javascript"></script>*@
    <link href="@Url.Content("~/Scripts/plugin/cellpagebar/pagebar.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/left.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery/jquery.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/common/json2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/plugin/jtemplates/jquery-jtemplates.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/plugin/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/common/common.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/common/js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/plugin/artDialog/jquery.artDialog.js?skin=idialog")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/plugin/artDialog/artDialogExt.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/plugin/cellpagebar/pagebar.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/views/manage/loginout.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/plugin/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    @RenderSection("SubHead", false)
    <script type="text/javascript">
        $(function () {
            $(".menu_title,.menu_title2").live("click", function () {
                var sid = $(this).attr("id");
                if ($("#submenu" + sid).css("display") == "none") {
                    $("#submenu" + sid).show();
                }
                else {
                    $("#submenu" + sid).hide();
                }
            });

            var parameters = {}; $.ajax.post("/DataManage/GetHeadMenu", parameters, function (data) {
                if (data.State) {
                    $("#divtopmenu").setTemplateURL("/Scripts/template/manage/head.htm");
                    $("#divtopmenu").processTemplate(data.Result);
                }
            });

            //            $(".clickoff").live("click", function () {
            //                $(".clickon").each(function () { $(this).attr("class", "clickoff") });
            //                $(this).attr("class", "clickon") 
            //            });

            $("#aLoginOut").click(function () {
                parameters = {};
                $.ajax.post("/Admin/LoginOut", parameters, function (data) {
                    if (data.State) {
                        window.location.href = "/Admin/Login";
                    }
                });
            });
        });
    </script>
</head>
<body style="margin: 0 auto; background-color: #E5F1FE; width: 100%; height: 100%">
    <div class="main">
        <div style="background-color: #0183C1; background-image: url('/Content/image/topbanner.jpg');
            background-repeat: no-repeat; clear: both; height: 88px;">
        </div>
        <div style="background-image: url('/Content/image/topbg.gif'); clear: both; height: 35px;">
            <div style="width: 100%; float: left" id="divtopmenu">
            </div>
        </div>
        <div id="Content">
            @RenderBody()
            <br style="clear: both;" />
        </div>
    </div>
</body>
</html>
