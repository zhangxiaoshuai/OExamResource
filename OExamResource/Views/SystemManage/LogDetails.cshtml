@{
    ViewBag.Title = "学校详细记录";
    Layout = "~/Views/Shared/_Back.cshtml";
}
<input type="hidden" name="hdTag" id="hdTag" value="@ViewBag.Tag" />
<input type="hidden" name="hdId" id="hdId" value="@Model.Id" />
<input type="hidden" name="hdSchoolId" value="@Model.Id"  id="hdSchoolId"/>
@section SubHead{
    <script src="@Url.Content("~/Scripts/views/manage/system.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/views/manage/schoollogdetails.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/plugin/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
}
<style type="text/css">
    #divLogList tr
    {
        height: 22px;
        text-align: center;
    }
    .date
    {
        width: 70px;
    }
</style>
@model Sync.Models.School 
<div id="divMenu" style="float: left; width: 158px">
</div>
<div id="divdata" style="margin-left: 160px">
    <table border="0" cellpadding="0" cellspacing="0" style="margin: 0 20px; width: 96%">
        <tr>
            <td colspan="2">
                <h2>@Model.Name</h2>
                 <a href="javascript:void();" id="AschoolFH" schoolid='@Model.Id'>返回</a>&nbsp;&nbsp;||&nbsp;&nbsp;<a
                    href="javascript:void();" id="aLogTxt">导出详细记录</a>
            </td>
        </tr>
        <tr>
            <td class="yan" valign="middle" align="left" style="height: 20px">
                <label id="lku">
                </label>
          记录列表
            </td>
            <td class="yan" valign="middle" align="right">
                共搜索出<strong id="sCount">@ViewBag.DownloadCount</strong>条记录
            </td>
        </tr>
        <tr>
            <td colspan="2">
                <div id="divLogList">
                </div>
            </td>
        </tr>
        <tr>
            <td colspan="2" align="center">
                <div id="divBar">
                </div>
            </td>
        </tr>
    </table>
    <div id="getTxtRM" style="display: none">
        <table border="0" cellpadding="10" cellspacing="10">
            <tr style="height: 20px">
                <td>
                    时间段：
                </td>
                <td>
                    <input type="text" name="RMstart" id="RMstart" onclick="WdatePicker()" value="@Model.CreateTime.ToString("yyyy-MM-dd")" class="date" />-
                    <input type="text" name="RMend" id="RMend" onclick="WdatePicker()" value="@DateTime.Now.ToString("yyyy-MM-dd")" class="date" />
                </td>
            </tr>
            <tr>
                <td>
                    选择类型：
                </td>
                <td>
                    <input type="radio" name="RMstatus" id="rRMPress" value="0" /><label for="rRMPress">浏览</label>
                    <input type="radio" name="RMstatus" id="rRMCollect" value="1" /><label for="rRMCollect">收藏</label>
                    <input type="radio" name="RMstatus" checked="checked" id="RMstatus" value="2" /><label
                        for="rDownload">下载</label>
                </td>
            </tr>
            <tr>
                <td>
                </td>
                <td>
                    <input type="button" name="btnGetTxtRM" id="btnGetTxtRM" value="导出" />
                </td>
            </tr>
        </table>
    </div>
    <div id="getTxtT" style="display: none">
        <table border="0" cellpadding="10" cellspacing="10">
            <tr style="height: 20px">
                <td>
                    时间段：
                </td>
                <td>
                    <input type="text" name="Tstart" id="Tstart" onclick="WdatePicker()" value="@Model.CreateTime.ToString("yyyy-MM-dd")" class="date" />-
                    <input type="text" name="Tend" id="Tend" onclick="WdatePicker()" value="@DateTime.Now.ToString("yyyy-MM-dd")" class="date" />
                </td>
            </tr>
            <tr>
                <td>
                    选择类型：
                </td>
                <td>
                    <input type="radio" name="Tstatus" id="rTPress" value="0" /><label for="rTPress">在线训练</label>
                    <input type="radio" name="Tstatus" checked="checked" id="rTDownload" value="2" /><label
                        for="rTDownload">下载组卷</label>
                </td>
            </tr>
            <tr>
                <td>
                </td>
                <td>
                    <input type="button" name="btnGetTxtT" id="btnGetTxtT" value="导出" />
                </td>
            </tr>
        </table>
    </div>
    <div id="getTxtE" style="display: none">
        <table border="0" cellpadding="10" cellspacing="10">
            <tr style="height: 20px">
                <td>
                    时间段：
                </td>
                <td>
                    <input type="text" name="Estart" id="Estart" onclick="WdatePicker()" value="@Model.CreateTime.ToString("yyyy-MM-dd")" class="date" />-
                    <input type="text" name="Eend" id="Eend" onclick="WdatePicker()" value="@DateTime.Now.ToString("yyyy-MM-dd")" class="date" />
                </td>
            </tr>
            <tr>
                <td>
                    选择类型：
                </td>
                <td>
                    <input type="radio" name="Estatus" id="rEPress" value="0" /><label for="rEPress">学习</label>
                    <input type="radio" name="Estatus" checked="checked" id="rEDownload" value="2" /><label
                        for="rEDownload">模考</label>
                </td>
            </tr>
            <tr>
                <td>
                </td>
                <td>
                    <input type="button" name="btnGetTxtE" id="btnGetTxtE" value="导出" />
                </td>
            </tr>
        </table>
    </div>
    <div id="getTxtQ" style="display: none">
        <table border="0" cellpadding="10" cellspacing="10">
            <tr style="height: 20px">
                <td>
                    时间段：
                </td>
                <td>
                    <input type="text" name="Qstart" id="Qstart" onclick="WdatePicker()" value="@Model.CreateTime.ToString("yyyy-MM-dd")" class="date" />-
                    <input type="text" name="Qend" id="Qend" onclick="WdatePicker()" value="@DateTime.Now.ToString("yyyy-MM-dd")" class="date" />
                </td>
            </tr>
            <tr>
                <td>
                </td>
                <td>
                    <input type="button" name="btnGetTxtQ" id="btnGetTxtQ" value="导出" />
                </td>
            </tr>
        </table>
    </div>
</div>
